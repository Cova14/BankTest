<h3>Añadir Banco</h3>
<form method="post" action="/bank/add">
    <label>Nombre del banco</label> <br>
    <input type="text" name="nombre">
    <br>
    <br> <br>
    <input type="submit" value="Submit">
</form>

<h3>Bancos</h3>
<ul>
<% bancos.forEach(function(banco){ %>
    <li><%= banco.nombre %></li>
<% }) %>
</ul>
<br>
<h3>Retiros</h3>

<table>
    <tr>
        <th>Retiros</th>
        <th>Fecha</th>
    </tr>
    <% retiros.forEach(function(retiro){ %>
        <tr>
            <th>$<%= retiro.cantidad %></th>
            <th><%= retiro.fecha %></th>
        </tr>
    <% }) %>
    <% retiros1.forEach(function(retiro1){ %>
        <tr>
            <th>$<%= retiro1.cantidad %></th>
            <th><%= retiro1.fecha %></th>
        </tr>
    <% }) %>
</table>

<h3>Retiros Totales</h3>
<h4><%= totalRetiros %> del día <%= fecha %></h4>
<h4><%= totalRetiros1 %> del día <%= fecha1 %></h4>

<a href="/graphic">Gráfica</a>

<br><br>
<form method="post" action="/file/add">
    <input type="submit" value="Importar">
</form>
